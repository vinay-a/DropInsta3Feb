<style name="wh_top_text_style" >

    <item name="android:gravity">center</item>
    <item name="android:background">@color/colorAccent</item>
    <item name="android:textColor">@color/white</item>
    <item name="android:padding">@dimen/layout_margin10dp</item>

</style>

    <style name="dash_text_style">
        <item name="android:textColor">@color/textColor</item>
        <item name="android:layout_alignParentLeft">true</item>

        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_marginLeft">@dimen/layout_margin10dp</item>
        <item name="android:textSize">@dimen/text_size21sp</item>

    </style>
    <style name="dash_count_style">
        <item name="android:textColor">@color/textColor</item>
        <item name="android:layout_alignParentRight">true</item>

        <item name="android:gravity">center_vertical|right</item>
        <item name="android:layout_marginRight">@dimen/layout_margin20dp</item>
        <item name="android:textSize">@dimen/text_size32sp</item>

    </style>
    <style name="payment_collection_style">
        <item name="android:maxLines">2</item>
        <item name="android:minLines">2</item>
        <item name="android:paddingTop">@dimen/layout_margin5dp</item>
        <item name="android:paddingBottom">@dimen/layout_margin5dp</item>
        <item name="android:paddingLeft">@dimen/layout_margin15dp</item>
        <item name="android:paddingRight">@dimen/layout_margin15dp</item>
        <item name="android:layout_marginBottom">2dp</item>
        <item name="android:gravity">center_horizontal|top</item>
        <item name="android:textSize">@dimen/text_size21sp</item>
    </style>


